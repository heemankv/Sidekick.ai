{
  "manifest_version": 3,
  "name": "My First Extension",
  "version": "1.0",
  "description": "A simple Chrome extension.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "logo.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "run_at": "document_start", 
      "js": ["jquery-3.5.0.js", "expanded.js", "popup.js"]
    }
  ],
  "permissions": []
}